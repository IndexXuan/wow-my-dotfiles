{
  "coc.preferences.hoverTarget": "float",
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "coc.preferences.formatOnType": false,
  "coc.preferences.bracketEnterImprove": true,
  "coc.preferences.snippetStatusText": "SNIP",
  "list.previewSplitRight": true,
  "list.normalMappings": {
    "<C-j>": "normal:j",
    "<C-k>": "normal:k",
    "<C-t>": "action:tabe"
  },
  "diagnostic.refreshAfterSave": true,
  "diagnostic.refreshOnInsertMode": false,
  "diagnostic.checkCurrentLine": true,
  "diagnostic.virtualTextPrefix": " ",
  "diagnostic.virtualText": false,
  "diagnostic.joinMessageLines": false,
  "codeLens.enable": true,
  "codeLens.separator": "‣",
  // make vim select first item on completion
  "suggest.noselect": false,
  // when snippet activated and pumvisible, prefer complete completion.
  "suggest.preferCompleteThanJumpPlaceholder": true,
  "suggest.minTriggerInputLength": 2,
  "suggest.acceptSuggestionOnCommitCharacter": true,
  "suggest.snippetIndicator": "~",
  "suggest.completionItemKindLabels": {
    "color": "",
    "class": "",
    "constant": "",
    "enum": "",
    "enumMember": "",
    "event": "鬒",
    "field": "綠",
    "folder": "",
    "function": "",
    "file": "",
    "interface": "禍",
    "module": "",
    "method": "",
    "operator": "洛",
    "property": "襁",
    "keyword": "",
    "reference": "",
    "snippet": "",
    "struct": "פּ",
    "text": "",
    "typeParameter": "",
    "unit": "",
    "variable": "",
    "value": "",
    "default": ""
	},
  "snippets.extends": {
    "vue": ["javascript", "typescript", "html", "css", "scss", "less"]
  },
  // coc-eslint
  "eslint.filetypes": ["javascript", "javascriptreact", "html", "vue", "jsx", "tsx"],
  // coc-prettier
  "prettier.eslintIntegration": true,
  "prettier.tslintIntegration": true,
  "prettier.stylelintIntegration": true,
  // coc-git  
  "git.branchCharacter": "",
  "git.changedDecorator": "*",
  "git.conflictedDecorator": "",
  "git.stagedDecorator": "",
  "git.untrackedDecorator": "",
  "git.virtualTextPrefix": "     ",
  "git.addGlameToVirtualText": true,
  "git.addGlameToBufferVar": true,
  // coc-emmet
  "emmet.includeLanguages": {
    "vue-html": "html",
    "javascript": "javascriptreact"
  },
  // languageserver
  "languageserver": {
    "bash": {
      "command": "bash-language-server",
      "args": ["start"],
      "filetypes": ["sh"],
      "ignoredRootPaths": ["~"]
    }
  }
}
