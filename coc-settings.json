{
  "hover.target": "float",
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "coc.preferences.formatOnType": false,
  "coc.preferences.bracketEnterImprove": true,
  // color highlight by coc-highlight
  "coc.preferences.colorSupport": true,
  "coc.preferences.rootPatterns": ["vite.config.ts", "vue.config.js", ".git"],
  "translator.engines": ["youdao"],
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "scss",
    "vue",
    "json",
    "javascript",
    "typescript",
    "dart"
  ],
  "list.previewSplitRight": true,
  "list.normalMappings": {
    "<C-j>": "normal:j",
    "<C-k>": "normal:k",
    "<C-t>": "action:tabe"
  },
  "javascript.suggestionActions.enabled": false,
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "wxss.validate": false,
  "diagnostic.refreshOnInsertMode": false,
  "diagnostic.checkCurrentLine": true,
  "diagnostic.virtualTextPrefix": " ",
  "diagnostic.virtualText": false,
  "codeLens.enable": false,
  "codeLens.separator": "‣",
  // make vim select first item on completion
  "suggest.noselect": false,
  // when snippet activated and pumvisible, prefer complete completion.
  "suggest.preferCompleteThanJumpPlaceholder": true,
  "suggest.minTriggerInputLength": 2,
  "suggest.acceptSuggestionOnCommitCharacter": false,
  "suggest.snippetIndicator": "~",
  "suggest.completionItemKindLabels": {
    "color": "",
    "class": "",
    "constant": "",
    "enum": "",
    "enumMember": "",
    "event": "鬒",
    "field": "綠",
    "folder": "",
    "function": "",
    "file": "",
    "interface": "禍",
    "module": "",
    "method": "",
    "operator": "洛",
    "property": "襁",
    "keyword": "",
    "reference": "",
    "snippet": "",
    "struct": "פּ",
    "text": "",
    "typeParameter": "",
    "unit": "",
    "variable": "",
    "value": "",
    "default": ""
  },
  "snippets.extends": {
    "vue": ["javascript", "typescript", "html", "css", "scss", "less"]
  },
  // coc-eslint
  "eslint.validate": ["javascript", "javascriptreact", "typescript", "html", "vue", "jsx", "tsx"],
  // coc-prettier
  // TODO: https://github.com/neoclide/coc-prettier/issues/62
  "prettier.eslintIntegration": false,
  "prettier.stylelintIntegration": true,
  // coc-git  
  "git.branchCharacter": "",
  "git.changedDecorator": "*",
  "git.conflictedDecorator": "",
  "git.stagedDecorator": "",
  "git.untrackedDecorator": "",
  "git.virtualTextPrefix": "     ",
  "git.addGBlameToVirtualText": true,
  "git.addGBlameToBufferVar": true,
  // vetur
  "vetur.enable": false,
  "vetur.ignoreProjectWarning": true,
  "vetur.useWorkspaceDependencies": true,
  "vetur.validation.template": false,
  "vetur.validation.interpolation": true,
  "vetur.validation.templateProps": true,
  "vetur.experimental.templateInterpolationService": true,
  // not working
  "vetur.underline.refValue": true,
  // volar
  "volar.enable": true,
  "volar.useWorkspaceTsdk": true,
  "volar.tagNameCase": "both",
  "volar.attrNameCase": "kebab",
  "volar.codeLens.references": true,
  "volar.codeLens.scriptSetupTools": true,
  // flutter
  "flutter.sdk.flutter-lookup": "asdf which flutter",
  "flutter.lsp.initialization.closingLabels": true,
  "flutter.closingLabelPrefix": "/. ",
  "flutter.provider.enableSnippet": true,
  "flutter.runDevToolsAtStartup": true,
  "flutter.autoOpenDevLog": true,
  "flutter.autoHideDevLog": true,
  "flutter.openDevLogSplitCommand": "botright 30vsplit"
}
